<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:background="@mipmap/registerbck"
	android:scrollbars="none">

	<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
		xmlns:app="http://schemas.android.com/apk/res-auto"
		xmlns:tools="http://schemas.android.com/tools"
		android:layout_width="match_parent"
		android:layout_height="match_parent"

		android:orientation="vertical"
		tools:context=".MainActivity">

		<include layout="@layout/toolbar_title" />

		<ImageView
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_gravity="center"
			android:layout_marginTop="@dimen/qb_px_90"
			android:src="@mipmap/ic_launcher" />

		<LinearLayout
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:layout_marginLeft="@dimen/qb_px_100"
			android:layout_marginTop="50dp"
			android:layout_marginRight="@dimen/qb_px_100"
			android:orientation="horizontal">

			<ImageView
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_gravity="center_vertical"
				android:src="@mipmap/imgphone" />

			<com.mingmen.canting.defineview.ClearEditText
				android:id="@+id/edit_Phone"
				android:layout_width="match_parent"
				android:layout_height="@dimen/qb_px_80"
				android:background="@null"
				android:hint="请输入您的手机号"
				android:inputType="phone"
				android:maxLength="11"
				android:paddingLeft="@dimen/qb_px_30"
				android:textColor="@color/white"
				android:textColorHint="@color/white"

				android:textSize="@dimen/qb_px_30" />
		</LinearLayout>

		<View
			android:layout_width="match_parent"
			android:layout_height="0.8dp"
			android:layout_marginLeft="@dimen/qb_px_80"
			android:layout_marginTop="@dimen/qb_px_20"
			android:layout_marginRight="@dimen/qb_px_80"
			android:background="@color/white" />

		<LinearLayout
			android:layout_width="match_parent"
			android:layout_height="@dimen/qb_px_128"
			android:layout_marginLeft="@dimen/qb_px_100"

			android:layout_marginRight="@dimen/qb_px_80"

			android:orientation="horizontal">

			<ImageView
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_gravity="center_vertical"
				android:layout_marginTop="@dimen/qb_px_15"
				android:src="@mipmap/imgpwd" />

			<com.mingmen.canting.defineview.ClearEditText
				android:id="@+id/edit_yanzheng"
				android:layout_width="0dp"
				android:layout_height="@dimen/qb_px_80"
				android:layout_gravity="center_vertical"
				android:layout_marginTop="@dimen/qb_px_15"
				android:layout_marginRight="15dp"
				android:layout_weight="1"
				android:background="@null"
				android:hint="请输入验证码"
				android:inputType="phone"
				android:maxLength="6"
				android:paddingLeft="@dimen/qb_px_30"
				android:textColor="@color/white"
				android:textColorHint="@color/white"
				android:textSize="@dimen/qb_px_30" />

			<Button
				android:id="@+id/btn_getcode"
				android:layout_width="wrap_content"
				android:layout_height="@dimen/qb_px_65"
				android:layout_gravity="center_vertical"
				android:layout_marginTop="@dimen/qb_px_15"
				android:background="@drawable/register_shape_yanzhengma"
				android:text="获取验证码"
				android:textColor="@color/white"
				android:textSize="@dimen/qb_px_30" />
		</LinearLayout>

		<View
			android:layout_width="match_parent"
			android:layout_height="0.7dp"
			android:layout_marginLeft="@dimen/qb_px_80"

			android:layout_marginRight="@dimen/qb_px_80"
			android:background="@color/white" />

		<LinearLayout
			android:layout_width="match_parent"
			android:layout_height="@dimen/qb_px_128"
			android:layout_marginLeft="@dimen/qb_px_100"

			android:layout_marginRight="@dimen/qb_px_80"

			android:orientation="horizontal">

			<ImageView
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_gravity="center_vertical"
				android:layout_marginTop="@dimen/qb_px_15"
				android:src="@mipmap/pwdregister" />

			<com.mingmen.canting.defineview.ClearEditText
				android:id="@+id/edit_pwd"
				android:layout_width="0dp"
				android:layout_height="@dimen/qb_px_80"
				android:layout_gravity="center_vertical"
				android:layout_marginTop="@dimen/qb_px_15"
				android:layout_marginRight="15dp"
				android:layout_weight="1"
				android:background="@null"
				android:hint="请输入您的登录密码"
				android:inputType="textPassword"
				android:maxLength="16"
				android:paddingLeft="@dimen/qb_px_30"
				android:textColor="@color/white"
				android:textColorHint="@color/white"
				android:textSize="@dimen/qb_px_30" />

			<LinearLayout
				android:layout_width="@dimen/qb_px_51"
				android:layout_height="match_parent"
				android:gravity="right|center_vertical"
				android:orientation="horizontal">

				<ImageView
					android:id="@+id/img_close_first"
					android:layout_width="wrap_content"
					android:layout_height="@dimen/qb_px_65"
					android:layout_gravity="center_vertical"
					android:layout_marginTop="@dimen/qb_px_15"
					android:src="@mipmap/closeeye"
					android:textColor="@color/white"
					android:textSize="@dimen/qb_px_28" />

				<ImageView
					android:id="@+id/img_open_first"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_gravity="center_vertical"
					android:layout_marginTop="@dimen/qb_px_15"
					android:background="@mipmap/openeye"
					android:visibility="visible" />
			</LinearLayout>
		</LinearLayout>

		<View
			android:layout_width="match_parent"
			android:layout_height="0.7dp"
			android:layout_marginLeft="@dimen/qb_px_80"

			android:layout_marginRight="@dimen/qb_px_80"
			android:background="@color/white" />

		<LinearLayout
			android:layout_width="match_parent"
			android:layout_height="@dimen/qb_px_128"
			android:layout_marginLeft="@dimen/qb_px_100"

			android:layout_marginRight="@dimen/qb_px_80"

			android:orientation="horizontal">

			<ImageView
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_gravity="center_vertical"
				android:layout_marginTop="@dimen/qb_px_15"
				android:src="@mipmap/pwdregister" />

			<com.mingmen.canting.defineview.ClearEditText
				android:id="@+id/edit_confirm"
				android:layout_width="0dp"
				android:layout_height="@dimen/qb_px_80"
				android:layout_gravity="center_vertical"
				android:layout_marginTop="@dimen/qb_px_15"
				android:layout_marginRight="15dp"
				android:layout_weight="1"
				android:background="@null"
				android:hint="请再次确认您的登录密码"
				android:inputType="textPassword"
				android:maxLength="16"
				android:paddingLeft="@dimen/qb_px_30"
				android:textColor="@color/white"
				android:textColorHint="@color/white"
				android:textSize="@dimen/qb_px_30" />

			<LinearLayout
				android:layout_width="@dimen/qb_px_51"
				android:layout_height="match_parent"
				android:gravity="right|center_vertical"
				android:orientation="horizontal">

				<ImageView
					android:id="@+id/img_close_second"
					android:layout_width="wrap_content"
					android:layout_height="@dimen/qb_px_65"
					android:layout_gravity="center_vertical"
					android:layout_marginTop="@dimen/qb_px_15"
					android:src="@mipmap/closeeye"
					android:textColor="@color/white"
					android:textSize="@dimen/qb_px_28"
					android:visibility="gone" />

				<ImageView
					android:id="@+id/img_open_second"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_gravity="center_vertical"
					android:layout_marginTop="@dimen/qb_px_15"
					android:background="@mipmap/openeye"
					android:visibility="gone" />

			</LinearLayout>
		</LinearLayout>


		<View
			android:layout_width="match_parent"
			android:layout_height="0.7dp"
			android:layout_marginLeft="@dimen/qb_px_80"

			android:layout_marginRight="@dimen/qb_px_80"
			android:background="@color/white" />

		<LinearLayout
			android:layout_width="match_parent"
			android:layout_height="@dimen/qb_px_128"
			android:layout_marginLeft="@dimen/qb_px_100"

			android:layout_marginRight="@dimen/qb_px_80"
			android:orientation="horizontal"
			android:visibility="visible">

			<ImageView
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_gravity="center_vertical"
				android:layout_marginTop="@dimen/qb_px_15"
				android:src="@mipmap/pwdregister" />

			<com.mingmen.canting.defineview.ClearEditText
				android:id="@+id/edit_name"
				android:layout_width="0dp"
				android:layout_height="@dimen/qb_px_80"
				android:layout_gravity="center_vertical"
				android:layout_marginTop="@dimen/qb_px_15"
				android:layout_marginRight="15dp"
				android:layout_weight="1"
				android:background="@null"
				android:hint="请输入您的姓名"
				android:inputType="text"
				android:maxLength="16"
				android:paddingLeft="@dimen/qb_px_30"
				android:textColor="@color/white"
				android:textColorHint="@color/white"
				android:textSize="@dimen/qb_px_30" />


		</LinearLayout>


		<View
			android:layout_width="match_parent"
			android:layout_height="0.7dp"
			android:layout_marginLeft="@dimen/qb_px_80"

			android:layout_marginRight="@dimen/qb_px_80"
			android:background="@color/white" />


		<LinearLayout
			android:layout_width="match_parent"
			android:layout_height="@dimen/qb_px_128"
			android:layout_marginLeft="@dimen/qb_px_100"

			android:layout_marginRight="@dimen/qb_px_80"

			android:orientation="horizontal">

			<ImageView
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_gravity="center_vertical"
				android:layout_marginTop="@dimen/qb_px_15"
				android:src="@mipmap/imgcanting" />

			<TextView
				android:id="@+id/text_canting"
				android:layout_width="0dp"
				android:layout_height="@dimen/qb_px_80"
				android:layout_gravity="center_vertical"
				android:layout_marginTop="@dimen/qb_px_15"
				android:layout_marginRight="15dp"
				android:layout_weight="1"
				android:background="@null"
				android:gravity="center_vertical"
				android:hint="请选择您的餐厅"
				android:inputType="text"
				android:maxLength="16"
				android:paddingLeft="@dimen/qb_px_30"
				android:textColor="@color/white"
				android:textColorHint="@color/white"
				android:textSize="@dimen/qb_px_30" />

			<ImageView
				android:id="@+id/img_cantingarrow"
				android:layout_width="wrap_content"
				android:layout_height="@dimen/qb_px_65"
				android:layout_gravity="center_vertical"
				android:layout_marginTop="@dimen/qb_px_15"
				android:layout_marginRight="20dp"
				android:src="@mipmap/img_arrowrigth"
				android:textColor="@color/white"
				android:textSize="@dimen/qb_px_28" />
		</LinearLayout>

		<View
			android:layout_width="match_parent"
			android:layout_height="0.7dp"
			android:layout_marginLeft="@dimen/qb_px_80"

			android:layout_marginRight="@dimen/qb_px_80"
			android:background="@color/white" />

		<LinearLayout
			android:layout_width="match_parent"
			android:layout_height="@dimen/qb_px_128"
			android:layout_marginLeft="@dimen/qb_px_100"

			android:layout_marginRight="@dimen/qb_px_80"

			android:orientation="horizontal">

			<ImageView
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_gravity="center_vertical"
				android:layout_marginTop="@dimen/qb_px_15"
				android:src="@mipmap/gangwei" />

			<TextView
				android:id="@+id/text_gangwei"
				android:layout_width="0dp"
				android:layout_height="@dimen/qb_px_80"
				android:layout_gravity="center_vertical"
				android:layout_marginTop="@dimen/qb_px_15"
				android:layout_marginRight="15dp"
				android:layout_weight="1"
				android:background="@null"
				android:gravity="center_vertical"
				android:hint="请选择您的岗位"
				android:inputType="text"
				android:maxLength="16"
				android:paddingLeft="@dimen/qb_px_30"
				android:textColor="@color/white"
				android:textColorHint="@color/white"
				android:textSize="@dimen/qb_px_30" />

			<ImageView
				android:id="@+id/img_ganweiarrow"
				android:layout_width="wrap_content"
				android:layout_height="@dimen/qb_px_65"
				android:layout_gravity="center_vertical"
				android:layout_marginTop="@dimen/qb_px_15"
				android:layout_marginRight="20dp"
				android:src="@mipmap/img_arrowrigth"
				android:textColor="@color/white"
				android:textSize="@dimen/qb_px_28" />
		</LinearLayout>

		<View
			android:layout_width="match_parent"
			android:layout_height="0.7dp"
			android:layout_marginLeft="@dimen/qb_px_80"

			android:layout_marginRight="@dimen/qb_px_80"
			android:background="@color/white" />

		<Button
			android:id="@+id/btn_register"
			android:layout_width="match_parent"
			android:layout_height="@dimen/qb_px_100"
			android:layout_marginLeft="@dimen/qb_px_80"
			android:layout_marginTop="@dimen/qb_px_60"
			android:layout_marginRight="@dimen/qb_px_80"
			android:background="@drawable/btnselector"
			android:text="注册"
			android:textColor="@color/white"
			android:textSize="16sp" />

		<LinearLayout
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:layout_marginLeft="@dimen/qb_px_80"
			android:layout_marginTop="@dimen/qb_px_20"
			android:layout_marginRight="@dimen/qb_px_80"
			android:gravity="center_vertical"
			android:orientation="horizontal">

			<CheckBox
				android:layout_width="15dp"
				android:layout_height="15dp"
				android:layout_gravity="center_vertical"
				android:layout_marginLeft="30dp"
				android:background="@drawable/checkbox_selector"
				android:button="@null"
				android:visibility="visible" />
			<!--   android:button="@drawable/register_pay_cb_bg"-->
			<!--    <CheckBox
                             android:id="@+id/check_register"
                             android:layout_width="@dimen/qb_px_24"
                             android:layout_height="@dimen/qb_px_24"
                             android:background="@drawable/register_cb_circle"
                             android:checked="true"
 android:background="@drawable/register_cb_circle"
                             /> -->
			<CheckBox
				android:id="@+id/check_register"
				android:layout_width="40dp"
				android:layout_height="40dp"
				android:button="@drawable/register_pay_cb_bg"
				android:buttonTint="@color/white"

				android:checked="true"
				android:visibility="gone" />

			<TextView
				android:id="@+id/text_register"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_gravity="center_vertical"
				android:layout_marginLeft="5dp"
				android:text="已阅读并同意"
				android:textColor="@color/white" />

			<TextView
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_marginLeft="5dp"
				android:text="《用户注册协议》"
				android:textColor="#AD041B"
				android:textSize="@dimen/qb_px_30"></TextView>
		</LinearLayout>

	</LinearLayout>
</ScrollView>

